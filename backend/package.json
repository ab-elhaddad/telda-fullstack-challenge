{
  "name": "movie-app-backend",
  "version": "1.0.0",
  "description": "Express Node.js API with TypeScript for Movie Application",
  "main": "dist/server.js",
  "scripts": {
    "dev": "nodemon --exec ts-node -r tsconfig-paths/register src/server.ts",
    "build": "tsc",
    "start": "node -r ./tsconfig-paths-bootstrap.js dist/server.js",
    "test": "jest --detectOpenHandles",
    "test:coverage": "jest --coverage",
    "lint": "eslint src/**/*.ts",
    "lint:fix": "eslint src/**/*.ts --fix",
    "format": "prettier --write 'src/**/*.{ts,js,json}'",
    "prepare": "cd .. && husky install backend/.husky",
    "seed-movies": "ts-node -r tsconfig-paths/register src/scripts/seed-movies.ts"
  },
  "keywords": [
    "express",
    "node",
    "typescript",
    "api",
    "movies"
  ],
  "author": "",
  "license": "ISC",
  "dependencies": {
    "@types/cookie-parser": "^1.4.9",
    "@types/multer": "^1.4.13",
    "@types/uuid": "^10.0.0",
    "bcryptjs": "^2.4.3",
    "compression": "^1.7.4",
    "cookie-parser": "^1.4.7",
    "cors": "^2.8.5",
    "dotenv": "^16.0.3",
    "express": "^4.18.2",
    "express-rate-limit": "^6.7.0",
    "helmet": "^6.1.5",
    "joi": "^17.9.2",
    "jsonwebtoken": "^9.0.0",
    "morgan": "^1.10.0",
    "multer": "^2.0.1",
    "pg": "^8.10.0",
    "uuid": "^11.1.0",
    "winston": "^3.8.2"
  },
  "devDependencies": {
    "@types/axios": "^0.14.4",
    "@types/bcryptjs": "^2.4.2",
    "@types/compression": "^1.7.2",
    "@types/cors": "^2.8.13",
    "@types/express": "^4.17.17",
    "@types/jest": "^29.5.1",
    "@types/jsonwebtoken": "^9.0.2",
    "@types/mocha": "^10.0.10",
    "@types/morgan": "^1.9.4",
    "@types/node": "^18.16.3",
    "@types/pg": "^8.6.6",
    "@types/supertest": "^2.0.12",
    "@typescript-eslint/eslint-plugin": "^5.59.2",
    "@typescript-eslint/parser": "^5.59.2",
    "axios": "^1.10.0",
    "eslint": "^8.39.0",
    "eslint-config-prettier": "^8.8.0",
    "eslint-plugin-prettier": "^4.2.1",
    "husky": "^8.0.3",
    "jest": "^29.5.0",
    "nodemon": "^2.0.22",
    "prettier": "^2.8.8",
    "supertest": "^6.3.3",
    "ts-jest": "^29.1.0",
    "ts-node": "^10.9.1",
    "tsconfig-paths": "^4.2.0",
    "typescript": "^5.0.4"
  }
}
